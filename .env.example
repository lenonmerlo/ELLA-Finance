# Backend local environment variables (DO NOT COMMIT .env)
# Copy this file to `backend/.env` and fill values.

# Resend
RESEND_API_KEY=
EMAIL_FROM=

# Database (required for local run)
# Neon example: jdbc:postgresql://<host>/<db>?sslmode=require
DB_URL=
DB_USERNAME=
DB_PASSWORD=

# JWT (optional override)
JWT_SECRET=

# CORS (required by application.properties)
# Example: http://localhost:5173
CORS_ALLOWED_ORIGINS=

# Optional: disable integrations locally
EMAIL_ENABLED=false
adobe.oauth.enabled=false
